# ============================================
# Agent Factory - Environment Variables
# ============================================
# Copy this file to .env and fill in your values
# ============================================

# --------------------------------------------
# AI Providers (Choose one or more)
# --------------------------------------------
# OpenAI - https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-...

# Anthropic - https://console.anthropic.com/settings/keys
ANTHROPIC_API_KEY=sk-ant-api03-...

# Google Gemini - https://aistudio.google.com/app/apikey
GOOGLE_GEMINI_API_KEY=AIza...
# For Gemini 3 Live API (WebSocket endpoint)
GEMINI_LIVE_WS_URL=wss://generativelanguage.googleapis.com/v1beta/models/gemini-3.0-live:streamGenerateContent

# --------------------------------------------
# Model Configuration (Optional)
# --------------------------------------------
# Default AI provider: 'openai', 'anthropic', or 'google'
AI_PROVIDER=openai

# Default model for Pattern 2
AI_MODEL=gpt-4o
# Options:
# - OpenAI: gpt-4o, gpt-4o-mini
# - Anthropic: claude-3-5-sonnet-20241022, claude-3-5-haiku-20241022
# - Google: gemini-2.0-flash, gemini-3.0-pro, gemini-3.0-live

# Pattern 3: Orchestrator model (handles complex coordination)
ORCHESTRATOR_MODEL=gpt-4o
# For Gemini Deep Think mode: gemini-3.0-pro with thinking_level=high

# Pattern 3: Sub-agent model (handles specific tasks)
SUB_AGENT_MODEL=gpt-4o-mini
# For fast Gemini: gemini-2.0-flash

# --------------------------------------------
# Gemini 3 Live API Configuration
# --------------------------------------------
# Thinking level: low, medium, high (deep think mode)
GEMINI_THINKING_LEVEL=medium

# Media resolution for video/screen capture: low, medium, high
GEMINI_MEDIA_RESOLUTION=medium

# Voice Activity Detection (VAD) settings
GEMINI_VAD_ENABLED=true
GEMINI_INTERRUPTION_THRESHOLD=0.5

# Enable grounding with Google Search
GEMINI_SEARCH_GROUNDING=true

# --------------------------------------------
# Pattern 4: MCP Server Integrations
# --------------------------------------------
# PostgreSQL Database
DATABASE_URL=postgresql://user:password@localhost:5432/dbname

# Redis Cache (Upstash) - https://console.upstash.com
REDIS_URL=redis://default:password@example.upstash.io:6379
UPSTASH_REDIS_REST_URL=https://example.upstash.io
UPSTASH_REDIS_REST_TOKEN=...

# Email Service (Resend) - https://resend.com/api-keys
RESEND_API_KEY=re_...

# Payment Processing (Stripe) - https://dashboard.stripe.com/apikeys
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# GitHub API - https://github.com/settings/tokens
GITHUB_TOKEN=ghp_...

# --------------------------------------------
# Pattern 5: Full Application
# --------------------------------------------
# Supabase - https://app.supabase.com/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=https://xxxxxxxxxxxxx.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJ...
SUPABASE_SERVICE_ROLE_KEY=eyJ...

# Clerk Auth - https://dashboard.clerk.com/apps/app_../instances/ins_../api-keys
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_...
CLERK_SECRET_KEY=sk_test_...

# Upstash Vector Database - https://console.upstash.com/vector
UPSTASH_VECTOR_URL=https://example.upstash.io
UPSTASH_VECTOR_TOKEN=...

# Upstash QStash (Background Jobs) - https://console.upstash.com/qstash
QSTASH_URL=https://qstash.upstash.io/v2/publish/
QSTASH_TOKEN=...
QSTASH_CURRENT_SIGNING_KEY=...
QSTASH_NEXT_SIGNING_KEY=...

# --------------------------------------------
# Optional Services
# --------------------------------------------
# Monitoring (Sentry) - https://sentry.io/settings/account/api/auth-tokens/
SENTRY_DSN=https://...@sentry.io/...
NEXT_PUBLIC_SENTRY_DSN=https://...@sentry.io/...

# Analytics (Vercel) - Automatic in Vercel deployments
# NEXT_PUBLIC_VERCEL_ANALYTICS_ID=...

# Logging (Axiom) - https://app.axiom.co/settings/tokens
AXIOM_TOKEN=xaat-...
AXIOM_ORG_ID=...

# S3 Storage (AWS or compatible) - https://aws.amazon.com/console/
AWS_ACCESS_KEY_ID=...
AWS_SECRET_ACCESS_KEY=...
AWS_REGION=us-east-1
S3_BUCKET_NAME=my-bucket

# --------------------------------------------
# Development Settings
# --------------------------------------------
# Node environment: 'development' or 'production'
NODE_ENV=development

# Port for local development
PORT=3000

# Enable debug logging
DEBUG=false

# --------------------------------------------
# NOTES
# --------------------------------------------
# 1. Never commit this file with real values
# 2. Add .env to .gitignore
# 3. Use different values for development/staging/production
# 4. Rotate keys regularly for security
# 5. Some services offer free tiers perfect for development:
#    - OpenAI: Pay as you go
#    - Anthropic: Pay as you go
#    - Google AI Studio: Free tier with rate limits
#    - Supabase: Free tier with 500MB database
#    - Upstash: Free tier with 10,000 requests/day
#    - Clerk: Free tier with 5,000 MAUs
#    - Resend: Free tier with 3,000 emails/month
#    - Stripe: No charges for test mode
# --------------------------------------------